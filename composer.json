{
    "name": "threef/entree",
    "description": "Threef Front End Extensions Orchestra Platform",
    "homepage": "",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "administrator", "admin", "user-management", "front"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Johari Jumali",
            "email": "joharijumali@gmail.com",
            "homepage": "https://bitbucket.org/joesama"
        }
    ],
    "autoload": {
        "psr-4": {
            "Threef\\Entree\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=5.6.4",
        "laracasts/utilities": "~2.0",
        "laravel/scout": "^1.1",
        "yajra/laravel-datatables-oracle": "~6.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^1.1",
        "guzzlehttp/guzzle": "~5.3|~6.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.3"
    },
    "suggest": {
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ],
        "update-frontend": [
            "bower update",
            "bower-installer",
            "gulp"
        ],
        "post-install-cmd": [
            "php artisan view:clear",
            "php artisan vendor:publish"
        ],
        "post-update-cmd": [
            "php artisan view:clear",
            "php artisan vendor:publish"
        ]
    },
    "extra": {
        "branch-alias": {
            "1.0": "1.0-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
