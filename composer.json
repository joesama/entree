{
    "name": "threef/entree",
    "description": "Front End Extensions Orchestra Platform",
    "homepage": "",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "administrator", "admin", "user-management", "front"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Johari Jumali",
            "email": "joharijumali@gmail.com",
            "homepage": "https://bitbucket.org/joesama"
        }
    ],
    "autoload": {
        "psr-4": {
            "Threef\\Entree\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=5.5.9",
        "orchestra/foundation": "3.2.*",
        "orchestra/installer": "~3.2",
        "knplabs/knp-snappy":"0.4.*",
        "maatwebsite/excel": "~2.1.0",
        "yajra/laravel-datatables-oracle":"~6.0",
        "caffeinated/menus": "~2.0",
        "laracasts/utilities": "~2.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.1",
        "orchestra/testing": "~3.2.0"
    },
    "suggest": {
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ],
        "update-frontend": [
            "bower update",
            "bower-installer",
            "gulp"
        ],
        "post-install-cmd": [
            "php artisan view:clear",
            "php artisan vendor:publish"
        ],
        "post-update-cmd": [
            "php artisan view:clear",
            "php artisan vendor:publish"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
