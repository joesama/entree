{
    "name": "joesama/entree",
    "description": "Entree Extensions Orchestra Platform",
    "homepage": "",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "administrator", "admin", "user-management", "front"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Johari Jumali",
            "email": "joharijumali@gmail.com",
            "homepage": "https://bitbucket.org/joesama"
        }
    ],
    "autoload": {
        "psr-4": {
            "Threef\\Entree\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=5.6.4",
        "orchestra/foundation": ">=3.4.0",
        "maatwebsite/excel": "~2.1.0",
        "laracasts/utilities": "~2.0",
        "intervention/image": "^2.4",
        "knplabs/knp-snappy": "^1.0",
        "laravel/scout": "^1.1",
        "laravel/passport": "^1.0",
        "teamtnt/laravel-scout-tntsearch-driver": "^1.1",
        "dimsav/laravel-translatable": "6.*",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "unisharp/laravel-ckeditor": "^4.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^2.3"
    },
    "suggest": {
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit"
        ],
        "update-frontend": [
            "bower update",
            "bower-installer",
            "gulp"
        ],
        "post-install-cmd": [
            "php artisan view:clear",
            "php artisan vendor:publish"
        ],
        "post-update-cmd": [
            "php artisan view:clear",
            "php artisan vendor:publish"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
